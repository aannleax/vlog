+	if (chaseMgmt->isCheckCyclicMode()) {
+	    if (blockedCount == sizecolumns) {
+		tmpRelation = std::unique_ptr<SegmentInserter>();
+		return;
+	    }
+	    for (size_t i = 0; i < sizecolumns; i++) {
+		if (blocked[i]) {
+		    for (int j = 0; j < atomTables.size(); j++) {
+			filterRows.push_back(i);
+		    }
+		}
+	    }
+	}
+
